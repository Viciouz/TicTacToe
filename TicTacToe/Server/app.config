<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="Metadata">
          <serviceMetadata />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="Metadata" name="Server.GameServer">
        <endpoint address="http://localhost:8080/femirad" binding="basicHttpBinding"
          bindingConfiguration="" contract="Shared.IGameServer" />
        <endpoint address="http://localhost:8080/femirad/mex" binding="mexHttpBinding"
          contract="IMetadataExchange" />
      </service>
    </services>
  </system.serviceModel>
</configuration>